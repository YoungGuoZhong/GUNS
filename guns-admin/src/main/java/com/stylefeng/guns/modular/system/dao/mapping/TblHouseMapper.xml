<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.TblHouseMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.TblHouse">
        <id column="id" property="id" />
        <result column="house_user" property="houseUser" />
        <result column="house_address" property="houseAddress" />
        <result column="house_date" property="houseDate" />
        <result column="house_desc" property="houseDesc" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, house_user AS houseUser, house_address AS houseAddress, house_date AS houseDate, house_desc AS houseDesc
    </sql>

</mapper>
